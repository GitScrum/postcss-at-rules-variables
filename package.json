{
  "name": "postcss-at-rules-variables",
  "version": "0.0.16",
  "description": "PostCss plugin to use CSS Custom Properties in at-rule @each, @for, @if, @else ",
  "main": "./lib/index.js",
  "scripts": {
    "test": "xo ./src/*.js ./test/*.js && mocha --compilers js:babel-register ./test/*.js",
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel --presets node5 src/index.es6.js --out-file lib/index.js",
    "prepublish": "npm run build",
    "coverage": "babel-node ./node_modules/isparta/bin/isparta cover _mocha --",
    "coveralls": "coveralls < coverage/lcov.info",
    "precoveralls": "npm run coverage",
    "update": "updtr"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "for",
    "if",
    "else",
    "each",
    "at-rules",
    "property"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GitScrum/postcss-at-rules-variables.git"
  },
  "author": {
    "name": "Scrum"
  },
  "license": "MIT",
  "dependencies": {
    "balanced-match": "^0.3.0",
    "lodash": "^4.0.0",
    "postcss": "^5.0.12"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-node5": "^10.8.0",
    "babel-register": "^6.7.2",
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "updtr": "^0.1.7",
    "xo": "^0.12.1"
  },
  "babel": {
    "presets": [
      "node5"
    ]
  },
  "bugs": {
    "url": "https://github.com/gitscrum/postcss-at-rules-variables/issues"
  },
  "homepage": "https://github.com/gitscrum/postcss-at-rules-variables#readme"
}
